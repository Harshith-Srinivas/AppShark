<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes" access="global" controller="ProductClass">
    <aura:attribute name="data" type="Object"/>
    <aura:attribute name="columns" type="List"/>
    <aura:handler name="init" value="{!this}" action="{!c.doinit}"/>
    <lightning:card variant="Narrow" title="Products" iconName="standard:product">
        </lightning:card>
    <lightning:datatable aura:id="prod-table"
                         columns="{!v.columns}"
                         data="{!v.data}"
                          keyField="Id"
                         />
</aura:component>