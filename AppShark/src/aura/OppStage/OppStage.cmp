<aura:component implements="flexipage:availableForRecordHome,force:hasRecordId" access="global" controller="OpportunityStage">
    <aura:attribute name="OppStage" type="String"/>
    <aura:attribute name="data" type="Object"/>
    <aura:attribute name="columns" type="List"/>
    <aura:handler name="init" value="{!this}" action="{!c.doinit}"/>
    <lightning:card>
    	<a onclick="{!c.navigateToOpp}" data-recid="Project Won">Project Won</a>
    	<a onclick="{!c.navigateToOpp}" data-recid="Project Lost">Project Lost</a>
    	<lightning:datatable aura:id="con-table"
                         columns="{!v.columns}"
                         data="{!v.data}"
                         keyField="Id"
                         />
    </lightning:card>
</aura:component>